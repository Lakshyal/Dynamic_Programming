#include <iostream>
#include <vector>
using namespace std;
vector<int> memoriztion;
int count{};
int fib(int n)
{
    // counter how many time by code run
    count++;
    if (n <= 2)
    {
        return 1;
    }
    // memorization part
    if (memoriztion[n] != -1)
        return memoriztion[n];
    // rec case
    return memoriztion[n] = fib(n - 1) + fib(n - 2);
}
int main()
{
#ifndef ONLINE_JUDGE
    freopen("input.txt", "r", stdin);
    freopen("output.txt", "w", stdout);
#endif
    int n;
    cin >> n;
    memoriztion.resize(n + 1, -1);
    cout << fib(n) << endl
         << count << endl;
    return 0;
}
